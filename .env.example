# PinkHouse Environment Variables

# API Keys (required for full functionality)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
SCRAPER_API_KEY=your_scraperapi_key_here

# Database (optional, defaults work for docker-compose)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/pinkhouse

# Redis (optional, defaults work for docker-compose)
REDIS_URL=redis://localhost:6379

# App Settings
DEBUG=true
